#include <stdio.h>
#include <stdlib.h>

/**
 * main - Beginning point
 *
 * Return: Always 0 (Successful)
 */
int main(void)
{
	char *pass = malloc(sizeof(char) * 5);
	char c[5] = {'H', 'o', 'l', 4, '\0'}, cmd[30];
	int x;

	for (x = 0; c[x]; x++)
		pass[x] = c[x];

	pass[x] = '\0';

	sprintf(cmd, "echo %s > 101-password", pass);
	system(cmd);

	return (0);
}
